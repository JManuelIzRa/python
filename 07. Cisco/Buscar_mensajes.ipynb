    {
      "cell_type": "code",
      "execution_count": 28,
      "metadata": {
        "id": "0dcdIffrMlOt"
      },
      "outputs": [],
      "source": [
        "resp = requests.get(   \"https://webexapis.com/v1/rooms\",\n",
        "                    headers = {\"Authorization\": accessToken}\n",
        "                )"
      ]
